# Realizar algunas operaciones utilizando los operadores relacionales definidos (todos ellos).

if (0 == 0) {
    print("0 == 0")
    if (1 != 0){
        print("1 != 0")
    } else {
        print("Error...")
    }
}

if (0 < 1){
    print("0 < 1")
    if(1 > 0){
        print("1 > 0")
    }
}

if(0 <= 1){
    print("0 <= 1")
    if(1 <= 1){
        print("1 <= 1")
    }
    if(1 >= 0){
        print("1 >= 0")
    }
    if(1 >= 1){
        print("1 >= 1")
    }
}