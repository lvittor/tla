# Imprimir el resultado de utilizar y combinar varias distribuciones de probabilidad.
main {

normal(0.0, 1.0) n1;
normal(2.0, 3.0) n2;

normal(n1 sumwith n2) n3;

normal(n1 sumwith n3) n4;

print("Normal n1: ");
print(n1);

print("Normal n2: ");
print(n2);

print("Normal n3: " + n3);
print("Normal n4: " + n4);

}