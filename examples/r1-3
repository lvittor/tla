# Imprimir el resultado de utilizar y combinar varias distribuciones de probabilidad.
main {

binomial(5, 0.5, 3) dist1;
normal(0.0, 1.0, -0.7) dist2;
poisson(3, 2) dist3;

if (dist1 > dist2) {
    print("Binomial case is more probable");
} else {
    print("Normal case is more probable");
}

if (dist2 > dist3) {
    print("Normal case is more probable");
} else {
    print("Poisson case is more probable");
}

if (dist3 > dist1) {
    print("Poisson case is more probable");
} else {
    print("Binomial case is more probable");
}

}